<Window x:Class="cs_new.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:cs_new"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
<Grid>
<StackPanel Margin="10">
<ListView ItemsSource="{Binding}" x:Name="listView" Margin="30,30,0,0">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Number" DisplayMemberBinding="{Binding Path=Node_num}" Width="50"/>
                    <GridViewColumn Header="x" DisplayMemberBinding="{Binding Path=Node_x}" Width="50"/>
                    <GridViewColumn Header="y" DisplayMemberBinding="{Binding Path=Node_y}" Width="50"/>
                    <GridViewColumn Header="from" DisplayMemberBinding="{Binding Path=Node_from}" Width="60"/>
                    <GridViewColumn Header="cost" DisplayMemberBinding="{Binding Path=Node_cost}" Width="60"/>
                </GridView>
            </ListView.View>
</ListView>
</StackPanel>
</Grid>
<!--
    <Grid>
        <DataGrid ItemsSource="{Binding Path=Data}" x:Name="dataGrid" />
    </Grid>
    <ComboBox>
        <ComboBox ItemsSource="{Binding Path=Data}" x:Name="comboBox" />
    </ComboBox>
-->
</Window>
